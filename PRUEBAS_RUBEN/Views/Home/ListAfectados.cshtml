@model IEnumerable<PRUEBAS.Models.Afectado>

@{
    ViewData["Title"] = "Lista Afectados";
}

<h1>Lista Afectados</h1>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Nombre</th>
            <th>DNI</th>
            <th>Teléfono</th>
            <th>Dirección</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var afectado in Model)
        {
            <tr>
                <td>@afectado.Nombre</td>
                <td>@afectado.DNI</td>
                <td>@afectado.Telefono</td>
                <td>@afectado.Direccion</td>
                <td>@afectado.Email</td>
            </tr>
        }
    </tbody>
</table>